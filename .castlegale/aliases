# Castlegale
export CASTLEGALE='/users/davecalnan/Code/castlegale-environmental/neptune'
export CG=$CASTLEGALE
export CGB="$CASTLEGALE/apps/backend"
export CGW="$CASTLEGALE/apps/web"

alias cg="cd $CG"
alias cgb="cd $CGB"
alias cgw="cd $CGW"

alias cgdev="osascript \"$(dirname "$0")/../.scripts/iterm-split-pane.scpt\" 'cd $CGW && pnpm dev' 'cd $CGB && php artisan serve'"
